                    <th>Name</th>
                    <th>Activity</th>
                    <th>Address</th>
                    <th>City</th>
                    <th>State</th>
                    <th>Zip</th>
                  </tr>
                </thead>
                <tbody>
                    <% for (var i = 0; i < Activity.length; i++) { %>
